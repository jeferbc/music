<div class="container">
  <div class="col-md-12">
    <div class="row">
      <% @artists.each do |artist|  %>
        <div class="col-sm-5 col-md-3">
          <div class="thumbnail">
            <img src="<%= artist.image_url %>" alt="" />
            <div class="caption">
              <h3><%= artist.name %></h3>
              <p><%= link_to 'Ver Mas', artist_path(id: artist.id), class: "btn btn-primary" %></p>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
